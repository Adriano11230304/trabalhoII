<%- include('../components/header.ejs')%>
    <div class="container">
        <h4 class="h4 m-2">Adicionar um novo usuário:</h4>
        <form class="py-3 col-5" action="/users/add" method="post">
            <div class="form-group">
                <label>CPF(Somente números)</label>
                <input type="text" class="form-control" maxlength="12" name="cpf" id="cpf">
            </div>
            <div class="form-group">
                <label>Nome</label>
                <input type="text" class="form-control" name="nome" id="nome">
            </div>
            <div class="form-group">
                <label>Password</label>
                <input type="password" class="form-control" name="password" id="password">
            </div>
            <div class="form-group">
                <label>Email</label>
                <input type="email" class="form-control" name="email" id="email">
            </div>
            <button class="btn btn-outline-dark" type="submit">Adicionar usuário</button>
        </form>
    </div>
    


<%- include('../components/footer.ejs')%>